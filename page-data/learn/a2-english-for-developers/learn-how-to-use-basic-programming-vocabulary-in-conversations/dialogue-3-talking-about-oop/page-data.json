    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 3: Talking about OOP","description":"<section id=\"description\">\n<p>Watch the video to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-use-basic-programming-vocabulary-in-conversations","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-use-basic-programming-vocabulary-in-conversations/dialogue-3-talking-about-oop","blockName":"learn-how-to-use-basic-programming-vocabulary-in-conversations","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-breakroom.png","characters":[{"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"5.1-3.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Tom","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Sarah","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":1,"finishTime":7.28,"dialogue":{"text":"Hey, Sarah, I was reading about object-oriented programming on the internet. It's a new concept for me.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":7.54,"finishTime":12.1,"dialogue":{"text":"Object-oriented programming is used in many modern languages. How much do you know about it so far?","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":12.7,"finishTime":18.12,"dialogue":{"text":"Not much, really. I understand that there are classes and methods, but I've never used them in my code.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":18.52,"finishTime":22.6,"dialogue":{"text":"No worries. Object-oriented programming is a fundamental concept in many languages.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":22.98,"finishTime":27.74,"dialogue":{"text":"It involves classes, which are like blueprints for objects, and inside the classes,","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":27.82,"finishTime":31.8,"dialogue":{"text":"you find the class's methods, which are actions those objects can perform.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":32.62,"finishTime":36.72,"dialogue":{"text":"That helps a lot. Can you recommend a good book for me to learn more about it?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":37.22,"finishTime":39.72,"dialogue":{"text":"Of course. I have a book on OOP's principles.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":40.2,"finishTime":42.9,"dialogue":{"text":"I'll send you the title. It has helped many people I know.","align":"right"}},{"background":null,"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":43.4,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":43.9,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-use-basic-programming-vocabulary-in-conversations","dashedName":"dialogue-3-talking-about-oop","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-use-basic-programm