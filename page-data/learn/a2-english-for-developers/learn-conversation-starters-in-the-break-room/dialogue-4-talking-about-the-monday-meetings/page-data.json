17.4,"dialogue":{"text":"We do. It's a chance to update everyone on project progress.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":17.6,"finishTime":20.1,"dialogue":{"text":"Awesome! Do they usually last long?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":20.8,"finishTime":23.8,"dialogue":{"text":"Not too long. They usually take about 30 to 45 minutes.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":24,"finishTime":27.5,"dialogue":{"text":"That's reasonable. Do we have an agenda for each meeting?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":27.7,"finishTime":31.2,"dialogue":{"text":"Yes, we do. Maria prepares the agenda in advance.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":31.4,"finishTime":35.2,"dialogue":{"text":"Good to know! Do the meetings involve everyone on the team?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":35.4,"finishTime":38.7,"dialogue":{"text":"Absolutely! All the team members have a chance to speak,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":38.7,"finishTime":40.4,"dialogue":{"text":"so everyone knows what the others are doing.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":40.6,"finishTime":45.1,"dialogue":{"text":"That's important. Does Maria assign tasks during these meetings?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":45.3,"finishTime":49.3,"dialogue":{"text":"Sometimes, but we normally discuss tasks we plan on tackling for the week.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":49.5,"finishTime":52,"dialogue":{"text":"Does this include brainstorming sessions?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":52.2,"finishTime":55.2,"dialogue":{"text":"Occasionally, yeah. It depends on the project's needs.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":55.5,"finishTime":58.3,"dialogue":{"text":"I can't wait for the first meeting I'll attend.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":58.5,"finishTime":59.5,"dialogue":{"text":"Probably next Monday.","align":"right"}},{"background":null,"character":"Sophie","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":60.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":61,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-conversation-starters-in-the-break-room","dashedName":"dialogue-4-talking-about-the-monday-meetings","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-conversation-starters-in-the-break-room","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-conversation-starters-in-the-break-room","nextChallengePath":"/learn/a2-english-for-developers/learn-conversation-starters-in-the-break-room/task-77","prevChallengePath":"/learn/a2-english-for-developers/learn-conversation-starters-in-the-break-room/task-76","id":"657cfff65708189adb524933"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":22,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-conversation-starters-in-the-break-room/dialogue-4-talking-about-the-monday-meetings"}},
