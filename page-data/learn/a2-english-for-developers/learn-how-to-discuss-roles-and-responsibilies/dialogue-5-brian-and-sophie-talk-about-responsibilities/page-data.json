12.8,"dialogue":{"text":"Tell me about your responsibilities beyond work.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":13.6,"finishTime":16.9,"dialogue":{"text":"Well, I have to make time for my family","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":15.6,"finishTime":18.9,"dialogue":{"text":"â€“ that's really important to me.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":17.6,"finishTime":21.6,"dialogue":{"text":"I also don't want to neglect my health,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":19.3,"finishTime":23.3,"dialogue":{"text":"so two months ago I started jogging regularly.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":21.8,"finishTime":23.4,"dialogue":{"text":"It helps me relax and stay fit.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":24.4,"finishTime":29,"dialogue":{"text":"It sounds like you're doing great dealing with work","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":26.5,"finishTime":31.1,"dialogue":{"text":"and life responsibilities at the same time.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":29,"finishTime":33.1,"dialogue":{"text":"Thank you. It's all about finding the right balance","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":31.9,"finishTime":36,"dialogue":{"text":"for you and your well-being.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":33.5,"finishTime":36.2,"dialogue":{"text":"And you don't have to search for expensive solutions for that.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":36.6,"finishTime":40.1,"dialogue":{"text":"Sometimes, it's the simple and free things","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":38.6,"finishTime":42.1,"dialogue":{"text":"that bring us the result we're looking for.","align":"right"}},{"background":null,"character":"Sophie","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":42.6,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":43.1,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-discuss-roles-and-responsibilies","dashedName":"dialogue-5-brian-and-sophie-talk-about-responsibilities","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-roles-and-responsibilies","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-discuss-roles-and-responsibilies","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-roles-and-responsibilies/task-88","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-roles-and-responsibilies/task-87","id":"65d9633ff2cc710bd3e18c03"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":19,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-roles-and-responsibilies/dialogue-5-brian-and-sophie-talk-about-responsibilities"}},
