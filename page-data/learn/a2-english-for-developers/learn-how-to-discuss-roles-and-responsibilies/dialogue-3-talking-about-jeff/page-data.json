    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 3: Talking about Jeff","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-roles-and-responsibilies","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-roles-and-responsibilies/dialogue-3-talking-about-jeff","blockName":"learn-how-to-discuss-roles-and-responsibilies","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-center.png","characters":[{"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"3.3-3.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Tom","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Maria","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":1,"finishTime":5,"dialogue":{"text":"I got this message saying I must talk to Jeff from the Security department.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":5.3,"finishTime":6.1,"dialogue":{"text":"Do you know him?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":7,"finishTime":11,"dialogue":{"text":"Yeah, I've seen him around.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":8.7,"finishTime":12.7,"dialogue":{"text":"He's responsible for security operations in our company.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":12,"finishTime":16.3,"dialogue":{"text":"He's been with us for some time, nearly ten years.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":14.8,"finishTime":19.1,"dialogue":{"text":"He's tall and has curly hair.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":16.6,"finishTime":19.7,"dialogue":{"text":"He always has this vigilant look, probably a result of his job.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":20.3,"finishTime":22,"dialogue":{"text":"He sounds like a dedicated professional.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":22.2,"finishTime":25.2,"dialogue":{"text":"I've always wondered what the folks in security do around here.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":25.5,"finishTime":26.9,"dialogue":{"text":"What are his responsibilities?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":27.6,"finishTime":34.2,"dialogue":{"text":"He makes sure the company's data and facilities are secure,","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":31.2,"finishTime":37.8,"dialogue":{"text":"monitors access controls, and conducts investigations when needed.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":35,"finishTime":37.7,"dialogue":{"text":"He also ensures that everyone follows security procedures.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":38.6,"finishTime":44.2,"dialogue":{"text":"Ah, speaking of procedures, is there anything he's told you","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":41.8,"finishTime":47.4,"dialogue":{"text":"that we need to do when it comes to office security?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":45.1,"finishTime":50.4,"dialogue":{"text":"Yes, there is one thing he emphasizes:","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":47.3,"finishTime":52.6,"dialogue":{"text":"you mustn't share your access cards or passwords with anyone.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":50.9,"finishTime":52.3,"dialogue":{"text":"Security is a big deal for him.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":53.1,"finishTime":54.8,"dialogue":{"text":"Understandable. Well, thanks.","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":55.4,"finishTime":58.6,"dialogue":{"text":"No problem. If you ever need any security tips,","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":57.6,"finishTime":60.8,"dialogue":{"text":"you really must talk to Jeff.","align":"right"}},{"background":null,"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":61.3,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":61.8,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-discuss-roles-and-responsibilies","dashedName":"dialogue-3-talking-about-jeff","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-roles-and-responsibilies","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-discuss-roles-and-r