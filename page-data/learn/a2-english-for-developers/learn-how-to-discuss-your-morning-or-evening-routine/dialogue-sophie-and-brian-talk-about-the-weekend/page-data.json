    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 5: Sophie and Brian Talk about the Weekend","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/dialogue-sophie-and-brian-talk-about-the-weekend","blockName":"learn-how-to-discuss-your-morning-or-evening-routine","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company1-reception.png","characters":[{"character":"Sophie","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Brian","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"2.2-5.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Sophie","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":1.2,"finishTime":4.8,"dialogue":{"text":"Hey Brian. TGIF, right? Do you have any plans for the weekend?","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":5,"finishTime":6.7,"dialogue":{"text":"Nothing out of the ordinary, Sophie.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":6.9,"finishTime":9.3,"dialogue":{"text":"On Saturday, I wake up early, at 7:00.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":9.4,"finishTime":11.6,"dialogue":{"text":"I run in the park near my place in the morning.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":11.9,"finishTime":15.2,"dialogue":{"text":"In the afternoon, I visit my parents and I have lunch with them.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":16,"finishTime":19.3,"dialogue":{"text":"Nice! On Saturday, I usually sleep until 9:30.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":19.8,"finishTime":21.6,"dialogue":{"text":"In the morning, I do some house cleaning.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":21.8,"finishTime":23.8,"dialogue":{"text":"In the afternoon, I watch a movie at home,","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":23.9,"finishTime":26.5,"dialogue":{"text":"or I go out with friends to do some window-shopping at the mall.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":26.7,"finishTime":27.8,"dialogue":{"text":"And on Sunday?","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":28.3,"finishTime":30.1,"dialogue":{"text":"On Sunday, I have a lazy morning.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":30.2,"finishTime":33,"dialogue":{"text":"I just stay at home and play some video games on my console.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":33.3,"finishTime":35.8,"dialogue":{"text":"In the evening, I make dinner and go to sleep early,","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":35.8,"finishTime":37.8,"dialogue":{"text":"because, on Monday, it's back to work again.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":38.2,"finishTime":40,"dialogue":{"text":"I have a relaxing Sunday, too.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":40.3,"finishTime":44.4,"dialogue":{"text":"In the morning and in the evening, I work on a personal coding project.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":44.5,"finishTime":48.1,"dialogue":{"text":"In the afternoon, I take a break and go for a quick walk in the park.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":48.8,"finishTime":51.1,"dialogue":{"text":"That's a good mix of indoor and outdoor activities!","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":51.4,"finishTime":53.6,"dialogue":{"text":"I don't work at the computer on weekends like you, though.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":53.7,"finishTime":55.5,"dialogue":{"text":"I save that for the weekdays, I suppose.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":55.7,"finishTime":56.9,"dialogue":{"text":"I know, right?","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":57,"finishTime":59.9,"dialogue":{"text":"But it is a personal project and I like it a lot,","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":59.9,"finishTime":62,"dialogue":{"text":"so I don't mind spending some of my time on it.","align":"right"}},{"background":null,"character":"Brian","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":62.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Sophie","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":63,"finishTime":null,"dialogue":null}]}}