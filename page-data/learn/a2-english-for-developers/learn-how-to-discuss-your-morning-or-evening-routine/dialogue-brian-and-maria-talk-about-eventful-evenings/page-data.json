    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 4: Brian and Maria Talk about Eventful Evenings","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/dialogue-brian-and-maria-talk-about-eventful-evenings","blockName":"learn-how-to-discuss-your-morning-or-evening-routine","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-parking.png","characters":[{"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"2.2-4.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Brian","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Maria","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":1.2,"finishTime":4,"dialogue":{"text":"Hey Maria, is it true that you're never home in the evenings?","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":4.6,"finishTime":5.5,"dialogue":{"text":"How do you do that?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":6.1,"finishTime":7.8,"dialogue":{"text":"Hi Brian. Well, yeah!","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":7.9,"finishTime":10.4,"dialogue":{"text":"I like doing fun things in the evening, so I go out a lot.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":11.3,"finishTime":14.4,"dialogue":{"text":"On Mondays, I have dance classes from 6 to 7:30.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":14.4,"finishTime":15.4,"dialogue":{"text":"I love dancing.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":15.5,"finishTime":17.6,"dialogue":{"text":"Oh, great! What about Tuesdays?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":18,"finishTime":19.8,"dialogue":{"text":"On Tuesdays, I learn Japanese.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":20.1,"finishTime":21.9,"dialogue":{"text":"I take online lessons at 7.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":22.2,"finishTime":24.5,"dialogue":{"text":"I think it's the only day I am home so early.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":24.9,"finishTime":27.6,"dialogue":{"text":"Then, on Wednesdays, I go to a local theater group.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":27.8,"finishTime":30.7,"dialogue":{"text":"Practice goes until 9 PM. We perform small plays.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":30.8,"finishTime":31.5,"dialogue":{"text":"It's really fun.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":31.7,"finishTime":34.2,"dialogue":{"text":"That's exciting! How about Thursdays?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":34.5,"finishTime":36.3,"dialogue":{"text":"Thursdays are for watching shows.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":36.4,"finishTime":38.8,"dialogue":{"text":"I look for live concerts or theater plays.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":38.8,"finishTime":40.5,"dialogue":{"text":"It's my favorite time to relax.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":40.8,"finishTime":43.6,"dialogue":{"text":"Wow! Do you rest at home on Fridays at least?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":43.8,"finishTime":46.2,"dialogue":{"text":"Not at all. I leave the weekends for that.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":46.5,"finishTime":48.8,"dialogue":{"text":"On Fridays, I like to hang out with friends.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":48.9,"finishTime":52.1,"dialogue":{"text":"We normally meet at a bar, chat and have something to eat and drink.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":52.3,"finishTime":55.8,"dialogue":{"text":"But I'm not really a night owl, so I like to be back home by 9:30 tops.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":56,"finishTime":59.4,"dialogue":{"text":"So many activities! I don't think I could keep up that pace.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":59.5,"finishTime":62.1,"dialogue":{"text":"But it's nice to know you enjoy it to the fullest, Maria.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":62.4,"finishTime":65.3,"dialogue":{"text":"I think I'll try to go out more often in the evening as well.","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":65.8,"finishTime":68.7,"dialogue":{"text":"Sure, Brian! Just don't forget to give yourself time to rest.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":68.8,"finishTime":71,"dialogue":{"text":"Find what your interests are and have some fun!","align":"right"}},{"background":null,"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":71.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":72,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-discuss-your-morning-or-evening-routine","dashedName":"dialogue-brian-and-maria-talk-about-eventful-evenings","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-discuss-your-morning-or-evening-routine","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/task-78","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/task-77","id":"655a9f8d6d3af8538a178166"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":22,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/dialogue-brian-and-maria-talk-about-eventful-evenings"}},
