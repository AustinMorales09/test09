    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 3: Evening Routine with Kids","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/dialogue-evening-routine-with-kids","blockName":"learn-how-to-discuss-your-morning-or-evening-routine","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company1-reception.png","characters":[{"character":"Jake","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"2.2-3.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Jake","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Sarah","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":1,"finishTime":5.2,"dialogue":{"text":"Hey Sarah. Sophie said you have some good tips for a great morning.","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":5.7,"finishTime":7.2,"dialogue":{"text":"I want to have a good evening routine, too,","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":8.2,"finishTime":10.7,"dialogue":{"text":"but it's complicated with my two kids running around.","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":10.9,"finishTime":12.9,"dialogue":{"text":"Do you have any ideas that could help?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":13.2,"finishTime":14.7,"dialogue":{"text":"I totally understand, Jake.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":14.9,"finishTime":15.7,"dialogue":{"text":"It can be tough.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":16,"finishTime":19.2,"dialogue":{"text":"Let's see if we can come up with some ideas that include the kids.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":19.4,"finishTime":22.1,"dialogue":{"text":"Great! I usually get home at around 6.","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":22.3,"finishTime":24.3,"dialogue":{"text":"After that, it's a bit chaotic.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":24.5,"finishTime":27.5,"dialogue":{"text":"You can start by involving your kids in light activities.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":27.9,"finishTime":29.3,"dialogue":{"text":"Go for a family walk","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":29.3,"finishTime":31.4,"dialogue":{"text":"or play a short outdoor game together when you get home.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":31.6,"finishTime":34.1,"dialogue":{"text":"It's a great way to have fun and stay active.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":34.3,"finishTime":36.4,"dialogue":{"text":"Nice! What about dinner?","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":36.4,"finishTime":37.1,"dialogue":{"text":"It's always a rush.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":37.3,"finishTime":40.5,"dialogue":{"text":"Plan simple and quick meals for weekdays.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":40.7,"finishTime":42.7,"dialogue":{"text":"Involve the kids in setting the table as well.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":42.9,"finishTime":46.6,"dialogue":{"text":"Good idea! After dinner, I usually help them with their homework.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":46.8,"finishTime":48,"dialogue":{"text":"That's important.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":48,"finishTime":51,"dialogue":{"text":"While they do homework, use the time to stretch or relax a bit.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":51.2,"finishTime":52.7,"dialogue":{"text":"This also helps with your self-care.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":52.9,"finishTime":56.4,"dialogue":{"text":"Amazing. Hmmâ€¦ How about bedtime?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":56.6,"finishTime":59.2,"dialogue":{"text":"Keep a consistent bedtime routine for your kids.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":59.5,"finishTime":63.3,"dialogue":{"text":"Read them a bedtime story or talk to them about nice, calm things at bedtime.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":63.6,"finishTime":67,"dialogue":{"text":"This helps them relax and gives you some quiet time after they go to sleep.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":67.5,"finishTime":68.5,"dialogue":{"text":"That makes sense!","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":68.8,"finishTime":71.1,"dialogue":{"text":"What do you do before going to sleep, Sarah?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":71.7,"finishTime":74.7,"dialogue":{"text":"I like to relax by reading a book or listening to music.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":75,"finishTime":76.6,"dialogue":{"text":"Sometimes, I meditate.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":76.9,"finishTime":79.1,"dialogue":{"text":"If your kids are asleep, you can do these things too.","align":"right"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":79.2,"finishTime":80.7,"dialogue":{"text":"Great suggestions, Sarah!","align":"left"}},{"background":null,"character":"Jake","position":null,"opacity":null,"startTime":80.9,"finishTime":83,"dialogue":{"text":"Thanks for helping me with my evening routine.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":83.5,"finishTime":84.3,"dialogue":{"text":"Anytime, Jake.","align":"right"}},{"background":null,"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":84.8,"finishTime":null,"dialogue":null},{"background":null,"character":"Jake","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":85.3,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-discuss-your-morning-or-evening-routine","dashedName":"dialogue-evening-routine-with-kids","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"supe