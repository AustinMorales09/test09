    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 2: Consulting a Wellness Expert","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/dialogue-consulting-a-wellness-expert","blockName":"learn-how-to-discuss-your-morning-or-evening-routine","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"interview-room1.png","characters":[{"character":"Sophie","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Expert","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"2.2-2.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Sophie","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Expert","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":1,"finishTime":4,"dialogue":{"text":"So, I'm here because I feel sleepy during the mornings.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":4.2,"finishTime":6.7,"dialogue":{"text":"I don't get productive until it's almost midday.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":6.7,"finishTime":9.2,"dialogue":{"text":"Do you have any tips on how to have a healthy morning routine?","align":"left"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":9.4,"finishTime":11.7,"dialogue":{"text":"Absolutely!","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":11.7,"finishTime":15.7,"dialogue":{"text":"My first suggestion is to not hit the snooze button multiple times.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":15.7,"finishTime":18.7,"dialogue":{"text":"It may be tempting, but it disrupts your sleep cycle","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":18.7,"finishTime":21.2,"dialogue":{"text":"and you may feel groggy during the day.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":21.2,"finishTime":23.8,"dialogue":{"text":"Set one alarm and get up when it rings.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":24,"finishTime":25.7,"dialogue":{"text":"That's a good point. What else?","align":"left"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":25.9,"finishTime":28.7,"dialogue":{"text":"Another thing: don't skip breakfast.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":28.7,"finishTime":31.7,"dialogue":{"text":"Breakfast gives you the energy to start the day.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":31.7,"finishTime":36.2,"dialogue":{"text":"If you skip it, you probably compensate later by eating too much","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":36.2,"finishTime":38.2,"dialogue":{"text":"or you feel sluggish in the morning.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":38.2,"finishTime":39.7,"dialogue":{"text":"Have a balanced breakfast instead.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":39.9,"finishTime":42.7,"dialogue":{"text":"I have a friend who does that in her routine.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":42.9,"finishTime":44.4,"dialogue":{"text":"Any other suggestions?","align":"left"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":44.6,"finishTime":48.2,"dialogue":{"text":"Yes, don't start your day by checking your phone or emails immediately.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":48.2,"finishTime":51.7,"dialogue":{"text":"It creates stress and makes you feel like you have to rush.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":51.7,"finishTime":54.2,"dialogue":{"text":"Take a few minutes for yourself.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":54.2,"finishTime":59.5,"dialogue":{"text":"For example, you can stretch or do some breathing exercises to clear your mind.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":59.7,"finishTime":60.9,"dialogue":{"text":"That's a good idea.","align":"left"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":62.1,"finishTime":65.3,"dialogue":{"text":"One more thing: don't rush during your morning routine.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":65.3,"finishTime":68.5,"dialogue":{"text":"Give yourself time to get ready without running around.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":68.5,"finishTime":72.5,"dialogue":{"text":"If you start the day feeling relaxed, you may have a more productive day.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":72.7,"finishTime":77.2,"dialogue":{"text":"Wow, thanks! I'll try to include these changes into my morning routine.","align":"left"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":77.7,"finishTime":81.5,"dialogue":{"text":"You're welcome! And remember: the small changes make a big difference.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":81.5,"finishTime":84.7,"dialogue":{"text":"Your morning starts well when you have a good night's sleep.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":84.7,"finishTime":89.5,"dialogue":{"text":"Don't forget to get enough sleep to feel well-rested in the morning.","align":"right"}},{"background":null,"character":"Expert","position":null,"opacity":null,"startTime":89.5,"finishTime":90.7,"dialogue":{"text":"Have a great morning!","align":"right"}},{"background":null,"character":"Expert","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":91.2,"finishTime":null,"dialogue":null},{"background":null,"character":"Sophie","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":91.7,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-discuss-your-morning-or-evening-routine","dashedName":"dialogue-consulting-a-wellness-expert","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-discuss-your-morning-or-evening-routine","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-discuss-your-morning-or-evening-routine","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/task-36","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-your-morning-or-evening-routine/task-35","id":"65593cf82e2