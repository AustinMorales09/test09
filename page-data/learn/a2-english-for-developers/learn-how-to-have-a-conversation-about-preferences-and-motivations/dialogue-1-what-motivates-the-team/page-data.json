    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 1: What Motivates the Team?","description":"<section id=\"description\">\n<p>Watch the video below to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-have-a-conversation-about-preferences-and-motivations","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/dialogue-1-what-motivates-the-team","blockName":"learn-how-to-have-a-conversation-about-preferences-and-motivations","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"cafe.png","characters":[{"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"4.1-1.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Brian","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Maria","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":1,"finishTime":4.5,"dialogue":{"text":"Hey Maria, I have a questionâ€¦ Do you know what motivates the team?","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":4.9,"finishTime":7.4,"dialogue":{"text":"How about you? What makes you feel motivated?","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":8.7,"finishTime":12.3,"dialogue":{"text":"Hi Brian! Well, one thing that really motivates me is learning new things.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":12.7,"finishTime":16,"dialogue":{"text":"I love exploring many new technologies and improving my skills.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":16.5,"finishTime":18.2,"dialogue":{"text":"It keeps me excited about our projects.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":19.7,"finishTime":20.9,"dialogue":{"text":"That's great to hear, Maria.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":21.3,"finishTime":24.7,"dialogue":{"text":"Learning and growing professionally motivates me a lot.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":25,"finishTime":27.2,"dialogue":{"text":"But you know what demotivates me sometimes?","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":27.7,"finishTime":32.6,"dialogue":{"text":"Dealing with many tight deadlines and feeling like I'm rushing to complete tasks.","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":33.1,"finishTime":37.3,"dialogue":{"text":"I can understand that, Brian. Racing against the clock can be demotivating.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":37.7,"finishTime":39.7,"dialogue":{"text":"Well, collaborating with our creative team","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":39.7,"finishTime":42.5,"dialogue":{"text":"and brainstorming many ideas really inspires me.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":42.8,"finishTime":44.6,"dialogue":{"text":"It's where we generate our best concepts.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":45.5,"finishTime":49,"dialogue":{"text":"I agree, Maria. Collaboration and creativity are great.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":49.3,"finishTime":53.6,"dialogue":{"text":"The problem is encountering many technical issues that slow our progress down.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":54,"finishTime":55.5,"dialogue":{"text":"It is really frustrating.","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":56.5,"finishTime":58.4,"dialogue":{"text":"Yeah, technical glitches are complicated","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":58.4,"finishTime":61,"dialogue":{"text":"and there's not much we can do other than try to solve them.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":62,"finishTime":63.3,"dialogue":{"text":"But do you know what else is great?","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":63.7,"finishTime":66.7,"dialogue":{"text":"Seeing the team's enthusiasm after solving these issues.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":67.1,"finishTime":69.8,"dialogue":{"text":"When you see the team inspired, it can boost your motivation.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":71.3,"finishTime":75.4,"dialogue":{"text":"Yeah. Wow. You know what? This was really a motivating talk.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":75.7,"finishTime":77.1,"dialogue":{"text":"Thanks for sharing your thoughts.","align":"left"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":77.7,"finishTime":79.8,"dialogue":{"text":"Anytime. It's good to have these discussions.","align":"right"}},{"background":null,"character":"Maria","position":null,"opacity":null,"startTime":80,"finishTime":81.6,"dialogue":{"text":"It reminds us of what keeps us going.","align":"right"}},{"background":null,"character":"Maria","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":82.1,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":82.6,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-have-a-conversation-about-preferences-and-motivations","dashedName":"dialogue-1-what-motivates-the-team","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-have-a-conversation-about-preferences-and-motivations","isFirstStep":true,"template":null,"required":[],"nextBlock":"learn-how-to-have-a-conversation-about-preferences-and-motivations","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/task-1","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-discuss-roles-and-responsibilies/task-106","id":"658111239f39a01c8f095c44"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":19,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/dialogue-1-what-motivates-the-team"}},
