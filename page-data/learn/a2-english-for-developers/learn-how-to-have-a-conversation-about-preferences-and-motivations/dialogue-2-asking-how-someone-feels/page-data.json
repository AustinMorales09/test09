    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 2: Asking How Someone Feels","description":"<section id=\"description\">\n<p>Watch the video below to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-have-a-conversation-about-preferences-and-motivations","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/dialogue-2-asking-how-someone-feels","blockName":"learn-how-to-have-a-conversation-about-preferences-and-motivations","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-roof.png","characters":[{"character":"Alice","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Tom","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"4.1-2.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Alice","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":2,"finishTime":5.6,"dialogue":{"text":"Hi Tom, you look a bit down these days. Is everything okay?","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":6.3,"finishTime":9.5,"dialogue":{"text":"Hey Alice, yeah, I feel a bit demotivated.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":9.8,"finishTime":13.6,"dialogue":{"text":"One thing that really gets to me is working long hours without any breaks.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":13.8,"finishTime":16.7,"dialogue":{"text":"It's exhausting, and it feels like I'm constantly running","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":16.7,"finishTime":18.1,"dialogue":{"text":"without knowing where I'm going.","align":"right"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":18.9,"finishTime":22.7,"dialogue":{"text":"I totally get that, Tom. Working overtime all the time can be draining.","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":23.4,"finishTime":25.7,"dialogue":{"text":"Personally, I hate dealing with office politics.","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":26.2,"finishTime":28.8,"dialogue":{"text":"It's frustrating when people are constantly trying to beat each other","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":28.8,"finishTime":30.6,"dialogue":{"text":"instead of working together as a team.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":31.3,"finishTime":34.7,"dialogue":{"text":"You're right, Alice. Dealing with office politics is never fun.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":35,"finishTime":38.4,"dialogue":{"text":"Another thing that gets to me is having too many meetings.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":38.9,"finishTime":41.7,"dialogue":{"text":"Sometimes, it feels like we're meeting for no reason,","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":41.7,"finishTime":44.3,"dialogue":{"text":"and it's hard to find time to actually work on projects.","align":"right"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":45,"finishTime":48.4,"dialogue":{"text":"I hear you, Tom. Sitting in meetings for hours can be discouraging.","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":49,"finishTime":52.6,"dialogue":{"text":"For me, what really annoys me is having to do a bunch of repetitive tasks","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":52.6,"finishTime":56.6,"dialogue":{"text":"without any variation. It feels like I'm wasting my skills and creativity.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":57.06,"finishTime":59,"dialogue":{"text":"That does sound frustrating, Alice.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":59.6,"finishTime":62.8,"dialogue":{"text":"Repeating the same tasks over and over can be demotivating.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":63.2,"finishTime":66.4,"dialogue":{"text":"Thanks for listening, by the way. It feels good to talk about these things.","align":"right"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":66.8,"finishTime":69.4,"dialogue":{"text":"Of course, Tom. We all have our moments of demotivation,","align":"left"}},{"background":null,"character":"Alice","position":null,"opacity":null,"startTime":69.4,"finishTime":71.3,"dialogue":{"text":"and it's good to share and support each other.","align":"left"}},{"background":null,"character":"Tom","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":71.8,"finishTime":null,"dialogue":null},{"background":null,"character":"Alice","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":72.3,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-have-a-conversation-about-preferences-and-motivations","dashedName":"dialogue-2-ask