    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 5: Discussing Open-source Software","description":"<section id=\"description\">\n<p>Watch the video below to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-have-a-conversation-about-preferences-and-motivations","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/dialogue-5-discussing-open-source-software","blockName":"learn-how-to-have-a-conversation-about-preferences-and-motivations","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-center.png","characters":[{"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Sophie","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"4.1-5.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Tom","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Sophie","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":1,"finishTime":3.5,"dialogue":{"text":"Hey, Sophie, what motivates you","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":3.8,"finishTime":6.9,"dialogue":{"text":"to work on open-source software projects in your free time?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":7.8,"finishTime":10.2,"dialogue":{"text":"Well, I love contributing to open-source projects!","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":10.5,"finishTime":13,"dialogue":{"text":"I really like the sense of community and collaboration.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":13.5,"finishTime":15.4,"dialogue":{"text":"It's incredible how people from around the world","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":15.4,"finishTime":17.5,"dialogue":{"text":"come together to create something amazing.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":18.2,"finishTime":20.4,"dialogue":{"text":"Yeah. I checked out some open-source projects,","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":20.5,"finishTime":23.1,"dialogue":{"text":"but I admit that I have problems with the amount of time","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":23.1,"finishTime":24.3,"dialogue":{"text":"it can take to contribute.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":24.7,"finishTime":27.1,"dialogue":{"text":"Sometimes, it can be so time-consuming.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":27.1,"finishTime":29.7,"dialogue":{"text":"I understand, Tom. It can be a commitment.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":29.9,"finishTime":33.3,"dialogue":{"text":"But I can't stand seeing this great software being private.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":33.8,"finishTime":36.5,"dialogue":{"text":"Open source is all about transparency and accessibility,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":36.5,"finishTime":38.4,"dialogue":{"text":"and that's something I truly believe in.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":39,"finishTime":42.2,"dialogue":{"text":"Yeah, transparency is a strong motivator for many developers.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":42.7,"finishTime":45.7,"dialogue":{"text":"For me, I like the idea of practicing my skills","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":45.7,"finishTime":48,"dialogue":{"text":"and getting recognition for my contributions.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":48.5,"finishTime":51.4,"dialogue":{"text":"It's a fantastic way to show what you're capable of.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":52.4,"finishTime":56,"dialogue":{"text":"Also, when software becomes obsolete because it's no longer maintained,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":56.4,"finishTime":58,"dialogue":{"text":"there isn't much you can do to save it.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":58.4,"finishTime":61.4,"dialogue":{"text":"With open source, there's always a chance for continuous improvement.","align":"right"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":62.2,"finishTime":65.2,"dialogue":{"text":"That's true. The sustainability of open-source projects","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":65.2,"finishTime":67.5,"dialogue":{"text":"is something I'm growing to appreciate more.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":67.8,"finishTime":69.2,"dialogue":{"text":"Thanks for sharing your motivations.","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":69.8,"finishTime":71.7,"dialogue":{"text":"I think I'll see if I can find a project","align":"left"}},{"background":null,"character":"Tom","position":null,"opacity":null,"startTime":71.7,"finishTime":72.8,"dialogue":{"text":"to dedicate some spare time to.","align":"left"}},{"background":null,"character":"Sophie","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":73.3,"finishTime":null,"dialogue":null},{"background":null,"character":"Tom","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":73.8,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-have-a-conversation-about-preferences-and-motivations","dashedName":"dialogue-5-discussing-open-source-software","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-have-a-conversation-about-preferences-and-motivations","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-have-a-conversation-about-preferences-and-motivations","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/task-113","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/task-112","id":"65a533f54a5afeff6c6bb996"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":19,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-how-to-have-a-conversation-about-preferences-and-motivations/dialogue-5-discussing-open-source-software"}},
