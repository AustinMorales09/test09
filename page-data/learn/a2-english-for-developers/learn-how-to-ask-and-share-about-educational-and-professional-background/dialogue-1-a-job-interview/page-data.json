acity":null,"startTime":10.9,"finishTime":12.3,"dialogue":{"text":"I was there for three years.","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":12.8,"finishTime":15.2,"dialogue":{"text":"I was responsible for developing web applications","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":15.3,"finishTime":17.3,"dialogue":{"text":"and maintaining the company's database.","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":17.9,"finishTime":19.9,"dialogue":{"text":"I was also part of the software update team.","align":"right"}},{"background":null,"character":"Anna","position":null,"opacity":null,"startTime":20.6,"finishTime":23.6,"dialogue":{"text":"That's excellent. Were there any specific projects you'd like to mention?","align":"left"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":23.9,"finishTime":26,"dialogue":{"text":"Yes, in one of my team's projects,","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":26.1,"finishTime":29.5,"dialogue":{"text":"we were involved in the development of an e-commerce platform.","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":29.8,"finishTime":32.2,"dialogue":{"text":"We were responsible for the checkout process,","align":"right"}},{"background":null,"character":"Candidate","position":null,"opacity":null,"startTime":32.3,"finishTime":34.4,"dialogue":{"text":"and it was a successful implementation.","align":"right"}},{"background":null,"character":"Anna","position":null,"opacity":null,"startTime":35.4,"finishTime":37.1,"dialogue":{"text":"Great! Can you tell me about your qualifications?","align":"left"}},{"background":null,"character":"Anna","position":null,"opacity":null,"startTime":37.5,"finishTime":39.1,"dialogue":{"text":"