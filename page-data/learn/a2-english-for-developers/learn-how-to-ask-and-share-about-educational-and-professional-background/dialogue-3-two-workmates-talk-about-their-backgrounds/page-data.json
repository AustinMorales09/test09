position":null,"opacity":null,"startTime":9.7,"finishTime":11.84,"dialogue":{"text":"I didn't study computer science at university,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":11.86,"finishTime":13.7,"dialogue":{"text":"but I majored in electrical engineering.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":14.4,"finishTime":16.4,"dialogue":{"text":"It wasn't exactly tech-related, but I learned a lot","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":16.44,"finishTime":18.4,"dialogue":{"text":"about problem-solving and critical thinking.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":18.9,"finishTime":19.5,"dialogue":{"text":"That's interesting!","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":20.1,"finishTime":23.06,"dialogue":{"text":"So, you didn't work on many programming projects","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":23.1,"finishTime":24.2,"dialogue":{"text":"during your studies, did you?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":25,"finishTime":25.8,"dialogue":{"text":"No, not really.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":26,"finishTime":29,"dialogue":{"text":"I played around with coding, but most of my projects involved","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":29.5,"finishTime":31,"dialogue":{"text":"circuit designs and electrical systems.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":31.2,"finishTime":32.2,"dialogue":{"text":"It was a bit different.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":33,"finishTime":34.9,"dialogue":{"text":"I see. Did you enjoy your time there,","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":35,"finishTime":37.2,"dialogue":{"text":"even if it wasn't entirely tech-focused?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":37.7,"finishTime":40.8,"dialogue":{"text":"I did. It was challenging, but I met some fantastic people.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":41.4,"finishTime":43.4,"dialogue":{"text":"We had a bunch of fun coding marathons,","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":43.5,"finishTime":45.3,"dialogue":{"text":"although I wasn't always a very strong programmer.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":45.9,"finishTime":47.5,"dialogue":{"text":"Well, we all have different journeys.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":48,"finishTime":52.2,"dialogue":{"text":"I studied at a small college as well and majored in information technology.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":52.7,"finishTime":55.6,"dialogue":{"text":"It was more hands-on, and I loved every bit of it.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":55.9,"finishTime":57.7,"dialogue":{"text":"We didn't have to wear too many hats,","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":58,"finishTime":59.8,"dialogue":{"text":"but I got to build and maintain networks","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":59.9,"finishTime":61.9,"dialogue":{"text":"and manage a few small projects.","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":62.5,"finishTime":63.4,"dialogue":{"text":"That sounds cool.","align":"right"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":63.7,"finishTime":66,"dialogue":{"text":"Did you ever work on any big projects during your studies?","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":67,"finishTime":68.8,"dialogue":{"text":"They weren't exactly big projects.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":69.2,"finishTime":72.9,"dialogue":{"text":"Most of them were smaller scale. But it was a great learning experience.","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":73.4,"finishTime":76.7,"dialogue":{"text":"It's fascinating to me how we all take different paths","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":76.8,"finishTime":79.4,"dialogue":{"text":"but end up in the same tech world, right?","align":"left"}},{"background":null,"character":"Sophie","position":null,"opacity":null,"startTime":80.2,"finishTime":82.8,"dialogue":{"text":"Absolutely! Let's grab a coffee sometime and chat more!","align":"right"}},{"background":null,"character":"Sophie","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":83.3,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":83.8,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-ask-and-share-about-educational-and-professional-background","dashedName":"dialogue-3-two-workmates-talk-about-their-backgrounds","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview