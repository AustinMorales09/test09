    "result": {"data":{"challengeNode":{"challenge":{"videoId":null,"title":"Dialogue 5: Brian Asks Sarah About Her Routine as a QA Tester","description":"<section id=\"description\">\n<p>Watch the video above to understand the context of the upcoming lessons.</p>\n</section>","challengeType":21,"helpCategory":"English","superBlock":"a2-english-for-developers","block":"learn-how-to-talk-about-a-typical-workday-and-tasks","fields":{"slug":"/learn/a2-english-for-developers/learn-how-to-talk-about-a-typical-workday-and-tasks/dialogue-5-brian-asks-sarah-about-her-routine-as-a-qa-tester","blockName":"learn-how-to-talk-about-a-typical-workday-and-tasks","tests":[]},"translationPending":false,"assignments":["<p>Watch the video</p>"],"scene":{"setup":{"background":"company2-breakroom.png","characters":[{"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null},{"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null}],"audio":{"filename":"2.1-5.mp3","startTime":1,"startTimestamp":null,"finishTimestamp":null},"alwaysShowDialogue":true},"commands":[{"background":null,"character":"Brian","position":{"x":25,"y":0,"z":1},"opacity":null,"startTime":0,"finishTime":null,"dialogue":null},{"background":null,"character":"Sarah","position":{"x":70,"y":0,"z":1},"opacity":null,"startTime":0.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":1,"finishTime":2,"dialogue":{"text":"Hey, Sarah!","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":2.7,"finishTime":3.9,"dialogue":{"text":"How's work as a QA tester?","align":"left"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":4.3,"finishTime":5.7,"dialogue":{"text":"Is it really challenging?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":6.5,"finishTime":9.2,"dialogue":{"text":"Sometimes. Let me tell you how my week usually goes.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":9.7,"finishTime":12.4,"dialogue":{"text":"Go on, please. What's your weekly routine like?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":12.9,"finishTime":16.9,"dialogue":{"text":"Well, on weekdays, I usually wake up at around 6 and get ready for work.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":17.4,"finishTime":19.8,"dialogue":{"text":"That's early! What's your morning routine?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":20,"finishTime":24,"dialogue":{"text":"In the morning, I have breakfast at home and then head to the office at 8.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":24.2,"finishTime":26.5,"dialogue":{"text":"What's the first thing you do when you arrive?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":26.7,"finishTime":30.7,"dialogue":{"text":"After I arrive, I check my emails and messages from the team.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":30.9,"finishTime":32.4,"dialogue":{"text":"It helps me plan my day.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":32.6,"finishTime":34.1,"dialogue":{"text":"Great. What's next?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":34.8,"finishTime":38,"dialogue":{"text":"After that, during the morning, I start testing the software.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":38.2,"finishTime":40.2,"dialogue":{"text":"I have a different task to do each day.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":40.4,"finishTime":42.9,"dialogue":{"text":"For example, on Mondays, I test the login process.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":42.9,"finishTime":46.9,"dialogue":{"text":"So, you test different parts of the software each day?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":47.1,"finishTime":51.1,"dialogue":{"text":"Exactly! At noon, I take a break and have lunch at the caf√© down the street.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":51.3,"finishTime":55,"dialogue":{"text":"Then, in the afternoon, I test other features, like the payment system.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":55.2,"finishTime":57.7,"dialogue":{"text":"I see. What else do you do during the week?","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":57.9,"finishTime":60.4,"dialogue":{"text":"On Wednesdays, we have a team meeting at 3.","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":60.6,"finishTime":64.3,"dialogue":{"text":"Next, I write test reports and document any bugs I found during the day.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":64.5,"finishTime":66.5,"dialogue":{"text":"Sounds like a very productive week!","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":66.7,"finishTime":68.7,"dialogue":{"text":"Yes, there are a lot of things,","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":68.7,"finishTime":71,"dialogue":{"text":"but I like collaborating to improve the quality of our software.","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":71.9,"finishTime":74.9,"dialogue":{"text":"Thanks for sharing, Sarah! It's interesting to know what you do.","align":"left"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":75.1,"finishTime":76.1,"dialogue":{"text":"You're welcome, Brian!","align":"right"}},{"background":null,"character":"Sarah","position":null,"opacity":null,"startTime":76.3,"finishTime":80,"dialogue":{"text":"When we have some time, I want to ask you about your week as a dev, okay?","align":"right"}},{"background":null,"character":"Brian","position":null,"opacity":null,"startTime":80.2,"finishTime":81,"dialogue":{"text":"Sure thing!","align":"left"}},{"background":null,"character":"Sarah","position":{"x":125,"y":0,"z":1},"opacity":null,"startTime":81.5,"finishTime":null,"dialogue":null},{"background":null,"character":"Brian","position":{"x":-25,"y":0,"z":1},"opacity":null,"startTime":82,"finishTime":null,"dialogue":null}]}}}},"pageContext":{"challengeMeta":{"blockHashSlug":"/learn/a2-english-for-developers/#learn-how-to-talk-about-a-typical-workday-and-tasks","dashedName":"dialogue-5-brian-asks-sarah-about-her-routine-as-a-qa-tester","certification":"a2-english-for-developers","disableLoopProtectTests":false,"disableLoopProtectPreview":false,"superBlock":"a2-english-for-developers","block":"learn-how-to-talk-about-a-typical-workday-and-tasks","isFirstStep":false,"template":null,"required":[],"nextBlock":"learn-how-to-talk-about-a-typical-workday-and-tasks","nextChallengePath":"/learn/a2-english-for-developers/learn-how-to-talk-about-a-typical-workday-and-tasks/task-109","prevChallengePath":"/learn/a2-english-for-developers/learn-how-to-talk-about-a-typical-workday-and-tasks/task-108","id":"657e6247e4566c108b138f35"},"projectPreview":{"showProjectPreview":false,"challengeData":{"challengeType":22,"challengeFiles":[]}},"slug":"/learn/a2-english-for-developers/learn-how-to-talk-about-a-typical-workday-and-tasks/dialogue-5-brian-asks-sarah-about-her