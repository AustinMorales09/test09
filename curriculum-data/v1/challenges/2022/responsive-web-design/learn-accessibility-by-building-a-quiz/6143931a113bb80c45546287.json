    {
      "text": "<p>You should place the <code>span</code> after the text content <code>D.O.B.</code>.</p>",
      "testString": "assert.match(document.querySelector('.info:nth-of-type(3) > label')?.innerHTML, /D\\.O\\.B\\.<span/);"
